<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".screen.Quanlydienthoai_Activity">
    <androidx.appcompat.widget.Toolbar
        app:title="Chi tiết đơn hàng"
        android:id="@+id/toolBarManHinhChiTietDonHang"
        app:titleTextColor="#0B0B0B"
        android:background="#46B9ED"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"></androidx.appcompat.widget.Toolbar>
    <ListView
        android:id="@+id/listViewManHinhChiTietDonHang"
        android:layout_marginTop="70dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"></ListView>
</LinearLayout>